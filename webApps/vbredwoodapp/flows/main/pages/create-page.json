  "eventListeners": {
    "confirmNoOjAction": {
      "chains": [
        {
          "chainId": "dialogResponseChain",
          "parameters": {
            "detail": "{{ $event.detail }}",
            "response": "NO"
          }
        }
      ]
    },
    "confirmYesOjAction": {
      "chains": [
        {
          "chainId": "dialogResponseChain",
          "parameters": {
            "detail": "{{ $event.detail }}",
            "response": "YES"
          }
        }
      ]
    },
    "ojdynamicFormTransientValueChangeChain": {
      "chains": [
